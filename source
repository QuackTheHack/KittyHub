-- Scripts:

local function CDCEY_fake_script()
	local script = Instance.new('LocalScript', ElcomeText)

	script.Parent.Text = "Welcome to KittyHub, ".. game.Players.LocalPlayer.Name
	wait(2.5)
	script.Parent.Parent.Visible = false
end
coroutine.wrap(CDCEY_fake_script)()
local function NOPDP_fake_script()
	local script = Instance.new('LocalScript', Buttons)

	wait(2.5)
	script.Parent.Visible = true
end
coroutine.wrap(NOPDP_fake_script)()
local function ODUHU_fake_script() 
	local script = Instance.new('LocalScript', Home)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Home.Visible = true
		local blur = Instance.new("BlurEffect", game.Lighting)
		game.Workspace.CurrentCamera.FieldOfView = 25
	end)
	
	script.Parent.Parent.Parent.Home.Close.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Home.Visible = false
		game.Lighting.Blur:Destroy()
		game.Workspace.CurrentCamera.FieldOfView = 70
	end)
end
coroutine.wrap(ODUHU_fake_script)()
local function MYATR_fake_script()
	local script = Instance.new('LocalScript', DiscordInvite)

	script.Parent.MouseButton1Click:Connect(function()
		print("https://discord.gg/NvqVhGwgJj")
	end)
end
coroutine.wrap(MYATR_fake_script)()
