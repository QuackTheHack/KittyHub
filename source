local KittyHub = Instance.new("ScreenGui")
local Welcome = Instance.new("Frame")
local ElcomeText = Instance.new("TextLabel")
local UICorner = Instance.new("UICorner")
local Buttons = Instance.new("Frame")
local Home = Instance.new("ImageButton")
local DiscordInvite = Instance.new("ImageButton")
local UICorner_2 = Instance.new("UICorner")
local Home_2 = Instance.new("Frame")
local Close = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")

--Properties:

KittyHub.Name = "KittyHub"
KittyHub.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Welcome.Name = "Welcome"
Welcome.Parent = KittyHub
Welcome.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
Welcome.Position = UDim2.new(0.402638197, 0, 0.00728155347, 0)
Welcome.Size = UDim2.new(0.194723621, 0, 0.0449029133, 0)

